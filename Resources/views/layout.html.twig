{% extends '::base.html.twig' %}

{% block content %}
    {% if app.request.hasPreviousSession %}
        {% for type, messages in app.session.flashbag.all() %}
            {% for message in messages %}
                <div class="flash-{{ type }} alert alert-{{ type }}" role="alert" >
                    <p>{{ message }}</p>
                </div>
            {% endfor %}
        {% endfor %}
    {% endif %}

    {% block stylesheets %}
        {{ parent() }}
    {% endblock %}
    {% block javascripts %}
        {{ parent() }}
    {% endblock %}
        <div class="container">
        {% block bh_seo_content %}
        {% endblock %}
        </div>

    {#{% block fos_user_content %}#}
    {#{% endblock fos_user_content %}#}
{% endblock %}